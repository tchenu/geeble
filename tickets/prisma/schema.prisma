datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Ticket {
  id   String @id @default(uuid())
  userId String
  eventId String
  data Json
  used Boolean
  createdAt DateTime @default(now())
}